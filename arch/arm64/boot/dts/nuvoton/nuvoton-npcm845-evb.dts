&i3c0 {
	status = "okay";
	i3c-scl-hz = <12500000>;
	i2c-scl-hz = <400000>;
	dmas = <&gdma0 0>, <&gdma0 1>;
	dma-names = "tx", "rx";
};

&i3c1 {
	status = "okay";
	i3c-scl-hz = <12500000>;
	i2c-scl-hz = <400000>;
};

&mc {
	status = "okay";
};

&pinctrl {
	pinctrl-names = "default";
	pinctrl-0 = <
			&jtag2_pins
			&lpc_pins
			&spix_pins
